  "description": "Bot Oficial de la Dodoria Army",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Yizack Rangel",
    "email": "yizackr@gmail.com",
    "url": "https://yizack.com"
  },
  "homepage": "https://dodoria.yizack.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yizack/dodoria.git"
  },
  "bugs": "https://github.com/Yizack/dodoria/issues",
  "scripts": {
    "build": "nuxt build",
    "build:local": "unbuild local",
    "dev": "nuxt dev --port=5173 --host",
    "dev:local": "jiti local/src/index",
    "generate": "nuxt generate",
    "preview": "wrangler d1 migrations apply DB --local && wrangler dev",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "register": "jiti scripts/register",
    "lint": "pnpm lint:eslint && pnpm lint:stylelint",
    "lint:fix": "pnpm lint:eslint --fix && pnpm lint:stylelint --fix",
    "lint:eslint": "eslint --config .config/eslint.mjs",
    "lint:eslint:inspect": "pnpm dlx @eslint/config-inspector --config .config/eslint.mjs",
    "lint:stylelint": "stylelint \"**/*.{css,scss,sass}\" --config .config/stylelint.json",
    "dev:cf": "set NITRO_PRESET=cloudflare-pages&&npm run build && wrangler pages dev dist",
    "test:types": "vue-tsc -b --noEmit",
    "start": "node local/build/index.mjs",
    "start:pm2": "pm2 start local/build/index.mjs --name dodoria",
    "db:migrate": "drizzle-kit generate --config=.config/drizzle.ts"
  },
  "devDependencies": {
    "@ahmedrangel/kient": "1.0.1-23",
    "@nuxt/eslint": "^1.9.0",
    "@nuxthub/core": "^0.9.0",
    "@resvg/resvg-wasm": "^2.6.2",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@whatwg-node/server": "^0.10.12",
    "@yeger/vue-masonry-wall": "^5.1.0",
    "bootstrap": "^5.3.8",
    "consola": "^3.4.2",
    "date-fns": "^4.1.0",
    "discord-api-types": "^0.38.22",
    "discord.js": "^14.22.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "eslint": "^9.34.0",
    "fast-levenshtein": "^3.0.0",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.4",
    "itty-router": "^5.0.22",
    "kient": "2.3.0",
    "music-metadata": "^11.8.3",
    "nuxt": "^4.1.0",
    "nuxt-auth-utils": "^0.5.23",
    "nuxt-twemoji": "^3.7.3",
    "nuxt-webhook-validators": "^0.2.3",
    "ohash": "^2.0.11",
    "otp": "^1.1.2",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "typescript": "^5.9.2",
    "ufo": "^1.6.1",
    "unbuild": "^3.6.1",
    "unwasm": "^0.3.11",
    "vue-tsc": "^3.0.6",
    "wrangler": "^4.34.0",
    "zod": "^4.1.5"
  },
  "packageManager": "pnpm@10.15.1"
}
