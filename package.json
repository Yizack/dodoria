    "bootstrap": "^5.3.3",
    "discord-interactions": "^4.0.0",
    "discord.js": "^14.15.3",
    "eslint": "^9.8.0",
    "nuxt": "^3.12.4",
    "nuxt-webhook-validators": "^0.0.6",
    "ohash": "^1.1.3",
    "stylelint": "^16.8.1",
    "stylelint-config-standard": "^36.0.1",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "ufo": "^1.5.4",
    "unwasm": "^0.3.9",
